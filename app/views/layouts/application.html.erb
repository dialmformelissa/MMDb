<!DOCTYPE html>
<html>
<head>
  <title>MMDb</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <link href='https://fonts.googleapis.com/css?family=Raleway|Vast+Shadow' rel='stylesheet' type='text/css'>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render "/movies/navbar" %>
  <% flash.each do |name, msg| %>
    <div class="<%= name %>">
      <%= msg %>
    </div>
  <% end %>


  <%= yield %>

